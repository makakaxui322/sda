AddPlr.MouseButton1Click:Connect(function()
	_G.Fire = true
	while _G.Fire == true and task.wait() do
		pcall(function()
			local bringed = TextBox.Text
			local snowballs = (game:GetService("Workspace")).Storage -- snowbaall storage
			local h = game.Players[bringed].Character.HumanoidRootPart
			for i, v in pairs(snowballs:GetDescendants()) do -- loops through all the snowballs
				if v.Name == "TouchInterest" and v.Parent then -- if touch interest and snowball exist
					local ball = v.Parent
					if game.Players.LocalPlayer.Character:FindFirstChild("ForceField") then -- if player has ff wait
						print("do nothing")
					else
						firetouchinterest(h, v.Parent, 0) -- fires stack

						firetouchinterest(h, v.Parent, 0) -- fires stack

						firetouchinterest(h, v.Parent, 0) -- fires stack

						firetouchinterest(h, v.Parent, 0) -- fires stack

						firetouchinterest(h, v.Parent, 0) -- fires stack

						firetouchinterest(h, v.Parent, 0) -- fires stack

					end
				end
			end
			if not game.Players.LocalPlayer.Character:FindFirstChild('ForceField') then
				task.wait(0.095)
				game.Players.LocalPlayer.Character:Destroy()
				game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
				task.wait(1)
			end
		end)
	end
end)

RemovePlr.MouseButton1Click:Connect(function()
	_G.Fire = false
end)

FFAbuse.MouseButton1Click:Connect(function()
	if FFAbuse.Text == "FF Abuse" then
		FFAbuse.Text = "Dont Working"
		_G.FF = true
	else
		FFAbuse.Text = "FF Abuse"
		_G.FF = false
	end
end)

AddPlr.MouseButton1Click:Connect(_G.addPlayer)
RemovePlr.MouseButton1Click:Connect(_G.RemovePlayer)

for i, v in pairs(game:GetService('Players'):GetChildren()) do
	if v ~= game.Players.LocalPlayer then
		for i2, v2 in pairs(v:GetChildren()) do
			if v2.Name == 'LocalSoundFolder' then
				v2:Destroy()
			end
		end
	end
end
