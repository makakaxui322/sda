	end
end)

rejoin.MouseButton1Click:Connect(function()
	local tps = game:GetService("TeleportService")
	local plr = game.Players.LocalPlayer
	tps:TeleportToPlaceInstance(game.PlaceId, game.JobId, plr)
end)

local UIS = game:GetService('UserInputService')
UIS.InputBegan:connect(function(input)
	if input.KeyCode == Enum.KeyCode.F1 then
		local j = game.Players.LocalPlayer.Character:GetPrimaryPartCFrame()
		game.ReplicatedStorage.RemoteEvent:FireServer({"Respawn"})
		task.wait(1.4)
		game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(j)
		task.wait()
		game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(j)
		task.wait()
		game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(j)
	end
end)

Dead.MouseButton1Click:Connect(function()
	if game.PlaceId == 2202352383 then
		game:GetService('Players').PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(function(character)
				character:WaitForChild("Humanoid").Died:Connect(function()
					game.StarterGui:SetCore( 
						"ChatMakeSystemMessage",  { 
							Text = player.Name .. " has died!ðŸ’€",
							Color = Color3.fromRGB(255, 0, 0), Font = Enum.Font.Arial, 
							FontSize = Enum.FontSize.Size24 
						}
					)
				end)
			end)
		end)
	end
end)

Join.MouseButton1Click:Connect(function()
	game:GetService('Players').PlayerAdded:Connect(function()
		wait()
		for i, v in pairs(game:GetService('Players'):GetChildren()) do
			if v ~= game.Players.LocalPlayer then
				for i2, v2 in pairs(v:GetChildren()) do
					if v2.Name == 'LocalSoundFolder' then
						v2:Destroy()
					end
				end
			end
		end
	end)
	local playerlist = {};
	for i,v in pairs(game.Players:GetPlayers())do
		table.insert(playerlist,v.Name)
	end
	function PlayerJoined(text)
		game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
			Text = text;
			Color = Color3.fromRGB(0, 85, 255);
			Font = Enum.Font.Oswald;
			TextSize = 25
		})
	end


	function PlayerLeft(text)
		game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
			Text = text;
			Color = Color3.fromRGB(255, 0, 0);
			Font = Enum.Font.Oswald;
			TextSize = 25
		})
	end


	game:GetService("Players").PlayerAdded:Connect(function(whojoined)
		PlayerJoined(" "..whojoined.Name.." joined the server")
		if whojoined.Name == "cat_miou" then
			PlayerJoined(" "..whojoined.Name.." Owner joined the server")
		elseif whojoined.Name == "stasaltakk" then
			PlayerJoined(" "..whojoined.Name.." Makaka joined the server")
		elseif whojoined.Name == "Rimzon31" then
			PlayerJoined(" "..whojoined.Name.." Staff  joined the server")


		end
	end)


	game:GetService("Players").PlayerRemoving:Connect(function(wholeft)
		PlayerLeft(" "..wholeft.Name.." left The Server")
		if wholeft.Name == "cat_miou" then
			PlayerJoined(" "..wholeft.Name.." Owner left the server")
		elseif wholeft.Name == "stasaltakk" then
			PlayerJoined(" "..wholeft.Name.." Makaka left the server")
		elseif wholeft.Name == "Rimzon31" then
			PlayerJoined(" "..wholeft.Name.." Staff left the server")
		end

	end)
end)

